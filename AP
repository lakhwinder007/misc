 //first term of ap= a, common diff=c  , check whether it is b is in ap sequence or not
 
 int inSequence(int a, int b, int c){
        // code here
        if(a==b)return 1;
        if(c==0&&a!=b)return 0;
        else if(c==0&&a==b)return 1;
       
       if((b-a)>0&&c>0)return (b-a)%c==0;
       if(b-a<0&&c<0)return (b-a)%c==0;
       
        else return 0;
    }
